{
    "name": "hexlet/code",
    "description": "Page loader",
    "type": "library",
    "license": "MIT",
    "bin": [
        "bin/page-loader"
    ],
    "autoload": {
        "psr-4": {
            "Downloader\\": "src/"
        },
        "files": [
            "src/Downloader.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alexey Kolvakh",
            "email": "alex.kolvah@mail.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.9",
        "imangazaliev/didom": "^2.0",
        "docopt/docopt": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "squizlabs/php_codesniffer": "^3.13"
    }
}
